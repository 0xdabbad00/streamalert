{
  "json_log": {
    "schema": {
      "name": "string",
      "host": "string",
      "data": {
        "time": "string"
      }
    },
    "parser": "json"
  },
  "syslog_log": {
    "schema": {
      "timestamp": "string",
      "host": "string",
      "application": "string",
      "message": "string"
    },
    "parser": "syslog"
  },
  "csv_log": {
    "schema": {
      "date": "string",
      "time": "integer",
      "host": "string",
      "message": "string",
      "source": "string"
    },
    "parser": "csv",
    "hints": {
      "source": [
        "cluster *"
      ]
    }
  },
  "kv_log": {
    "schema": {
      "type": "string",
      "msg": "string",
      "uid": "integer"
    },
    "parser": "kv",
    "delimiter": " ",
    "separator": "="
  }
}